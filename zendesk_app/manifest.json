{
  "name": "Quivr",
  "author": {
    "name": "quivr",
    "email": "antoine@quivr.app",
    "url": "https://www.quivr.com"
  },
  "defaultLocale": "en",
  "domainWhitelist": [
    "api.quivr.app",
    "api-gobocom.quivr.app",
    "api-preview.quivr.app"
  ],
  "private": false,
  "location": {
    "support": {
      "ticket_sidebar": {
        "url": "assets/index.html",
        "flexible": true
      },
      "ticket_editor": {
        "url": "assets/editor.html"
      },
      "modal": {
        "url": "assets/modal.html"
      }
    }
  },
  "parameters": [
    {
      "name": "quivr_api_token",
      "type": "text",
      "required": true,
      "secure": false
    },
    {
      "name": "quivr_workspace_id",
      "type": "text",
      "required": true,
      "secure": false
    },
    {
      "name": "zendesk_api_key",
      "type": "text",
      "required": true,
      "secure": true
    }
  ],
  "version": "1.10.6",
  "frameworkVersion": "2.0"
}
