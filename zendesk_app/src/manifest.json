{
  "name": "Quivr",
  "author": {
    "name": "quivr",
    "email": "founders@quivr.app",
    "url": "https://www.quivr.com"
  },
  "defaultLocale": "en",
  "domainWhitelist": [
    "api.quivr.app",
    "api-gobocom.quivr.app",
    "api-preview.quivr.app"
  ],
  "private": false,
  "location": {
    "support": {
      "ticket_sidebar": {
        "url": "http://localhost:3000",
        "flexible": true
      },
      "ticket_editor": {
        "url": "http://localhost:3000/editor"
      },
      "modal": {
        "url": "http://localhost:3000/modal"
      }
    }
  },
  "parameters": [
    {
      "name": "quivr_api_token",
      "type": "text",
      "required": true,
      "secure": false
    },
    {
      "name": "zendesk_api_key",
      "type": "text",
      "required": true,
      "secure": true
    }
  ],
  "version": "1.14.1",
  "frameworkVersion": "2.0"
}